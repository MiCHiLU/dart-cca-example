!!!
%html/
%meta(name="viewport" content="width=device-width, initial-scale=1, user-scalable=no")
%title paper-input
%script(src="packages/web_components/platform.js")
%script(src="packages/web_components/dart_support.js")
%link(rel="import" href="packages/polymer/polymer.html")
%link(rel="import" href="poly_element.html")
%link(rel="import" href="packages/paper_elements/roboto.html")
%link(rel="import" href="paper_element.html")
%style(shim-shadowdom)
  .ng-show {
  display: block !important;
  }
  [ng-cloak], [data-ng-cloak], [x-ng-cloak], .ng-cloak, .x-ng-cloak, .ng-hide {
  display: none !important;
  }
  body {
  font-family: RobotoDraft, 'Helvetica Neue', Helvetica, Arial;
  margin: 0;
  padding: 24px;
  \-webkit-user-select: none;
  \-moz-user-select: none;
  \-ms-user-select: none;
  user-select: none;
  \-webkit-tap-highlight-color: rgba(0,0,0,0);
  \-webkit-touch-callout: none;
  }
%script(src="main.dart" type="application/dart")
%body(ng-app ng-cloak unresolved touch-action="auto")/

%section
  %h4 Angular
  %p
    %input(type="text" ng-model="cool")
    Angular binding: {{cool}}
%section
  %h4 Polymer
  %poly-element(message="[[ cool ]]")

%section
  %h4 Paper Elements
  %paper-input(floatingLabel label="Paper Elements")
